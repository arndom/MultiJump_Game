{
  "postGameScreen": {
    "showPostGameScreen": true,
    "showPlayAgainButton": true,
    "playAgainButtonText": "Play Again",
    "playAgainButtonFontSize": 20,
    "playAgainButtonColor": "#c31919",
    "title": "Thanks for Playing",
    "actions": {
      "action": "reveal",
      "emailCollection": "yes",
      "phoneCollection": "no",
      "optInText": "I agree to be contacted",
      "submitButtonText": "Submit",
      "submitButtonColor": "#2d179b",
      "viewLeaderboardButtonText": "View Leaderboard",
      "viewLeaderboardButtonColor": "#2f1f7d",
      "text": "Create a game on Koji in 5 minutes!",
      "buttonText": "Get Started",
      "buttonLink": "https://withkoji.com",
      "leaderboardColor": "#361db3",
      "leaderboardBackgroundColor": "#5a4d9c",
      "buttonColor": "#b4b933",
      "winText": "Congratulations, you won!",
      "loseText": "Good try, better luck next time!",
      "message": "Visit <a href=\"https://withkoji.com\">our site</a> to learn more."
    },
    "reveal": "bounceBottom",
    "leaderboardFields": [
      {
        "label": "First Name"
      }
    ],
    "leaderboardEnabled": true,
    "ctaEnabled": true,
    "ctaHeadline": "Check out our site!",
    "socialShareEnabled": true,
    "socialShareHeadline": "Invite Friends",
    "playAgainLinkEnabled": true,
    "playAgainLinkText": "",
    "facebookIcon": "https://images.koji-cdn.com/4fba5aca-3a39-409c-8db0-c660207a39ee/ovg2x-roundedfacebook.png",
    "linkedInIcon": "https://images.koji-cdn.com/4fba5aca-3a39-409c-8db0-c660207a39ee/qlyjr-roundedlinkedin.png",
    "twitterIcon": "https://images.koji-cdn.com/4fba5aca-3a39-409c-8db0-c660207a39ee/o7q85-roundedtwitter.png",
    "redditIcon": "https://images.koji-cdn.com/4fba5aca-3a39-409c-8db0-c660207a39ee/kovap-roundedreddit.png",
    "collectEmail": "no",
    "collectPhone": "no",
    "optInText": "I agree to receive updates"
  },
  "@@editor": [
    {
      "key": "postGameScreen",
      "name": "3. End Card",
      "source": "postGameScreen.json",
      "fields": [
        {
          "key": "showPostGameScreen",
          "name": "Show End Card",
          "type": "boolean"
        },
        {
          "key": "leaderboardEnabled",
          "name": "LEADERBOARD",
          "type": "boolean"
        },
        {
          "key": "collectEmail",
          "name": "Collect Email",
          "type": "select",
          "typeOptions": {
            "placeholder": "Choose an option...",
            "options": [
              {
                "value": "no",
                "label": "Do Not Collect"
              },
              {
                "value": "yes",
                "label": "Collect"
              },
              {
                "value": "require",
                "label": "Collect & Require For Submission"
              }
            ]
          }
        },
        {
          "key": "collectPhone",
          "name": "Collect Phone",
          "type": "select",
          "typeOptions": {
            "placeholder": "Choose an option...",
            "options": [
              {
                "value": "no",
                "label": "Do Not Collect"
              },
              {
                "value": "yes",
                "label": "Collect"
              },
              {
                "value": "require",
                "label": "Collect & Require For Submission"
              }
            ]
          }
        },
        {
          "key": "optInText",
          "name": "Opt In Text",
          "type": "text"
        },
        {
          "key": "leaderboardWebhookURL",
          "name": "Zapier Webhook URL",
          "description": "If you have a Zapier integration, you can connect with their webhook service. Watch a tutorial here: https://withkoji.com",
          "type": "text"
        },
        {
          "key": "ctaEnabled",
          "name": "CALL TO ACTION",
          "type": "boolean"
        },
        {
          "key": "ctaHeadline",
          "name": "CTA Headline",
          "type": "text"
        },
        {
          "key": "ctaButtonText",
          "name": "CTA Button Text",
          "type": "text"
        },
        {
          "key": "ctaButtonLink",
          "name": "CTA Button Link",
          "type": "text"
        },
        {
          "key": "ctaButtonColor",
          "name": "CTA Button Color",
          "type": "color"
        },
        {
          "key": "socialShareEnabled",
          "name": "SOCIAL SHARE",
          "type": "boolean"
        },
        {
          "key": "socialShareHeadline",
          "name": "Social Share Headline",
          "type": "text"
        },
        {
          "key": "playAgainLinkEnabled",
          "name": "Show Play Again Link",
          "type": "boolean"
        },
        {
          "key": "reveal",
          "name": "Reveal Animation",
          "description": "How the pre game screen reveal is animated",
          "type": "select",
          "typeOptions": {
            "placeholder": "Choose an option...",
            "options": [
              {
                "label": "Bounce Bottom",
                "value": "bounceBottom"
              },
              {
                "label": "Bounce Top",
                "value": "bounceTop"
              },
              {
                "label": "Fade Bottom",
                "value": "fadeBottom"
              },
              {
                "label": "Fade Top",
                "value": "fadeTop"
              },
              {
                "label": "Zoom Bottom",
                "value": "zoomBottom"
              },
              {
                "label": "Zoom Top",
                "value": "zoomTop"
              },
              {
                "label": "Rubber Band",
                "value": "rubberBand"
              }
            ]
          }
        }
      ]
    }
  ]
}