  "@@editor": [
    {
      "key": "general",
      "name": " Global Settings",
      "source": "general.json",
      "fields": [
        {
          "key": "fontFamily",
          "name": "Google Font Family",
          "description": "Visit https://fonts.google.com, and copy and paste the name only (e.g., Roboto)",
          "type": "text"
        },
        {
          "key": "backgroundImage",
          "name": "Background Image",
          "type": "image"
        },
        {
          "key": "soundOnIcon",
          "name": "Sound On Icon",
          "type": "image"
        },
        {
          "key": "soundOffIcon",
          "name": "Sound Off Icon",
          "type": "image"
        },
        {
          "key": "debug",
          "name": "Testing/Debug Options",
          "description": "You can use these to quickly test your game",
          "type": "object<Debug>",
          "typeOptions": {
            "Debug": {
              "startScreen": {
                "name": "Start Screen",
                "description": "Note: Helpful for testing your updates, make sure to set back to \"Home Screen\" before publishing!",
                "type": "select",
                "typeOptions": {
                  "placeholder": "Choose an option...",
                  "options": [
                    {
                      "label": "Pre Game Screen",
                      "value": "preGame"
                    },
                    {
                      "label": "Game Screen",
                      "value": "game"
                    },
                    {
                      "label": "Post Game Screen",
                      "value": "postGame"
                    }
                  ]
                }
              }
            }
          }
        }
      ]
    }
  ]
